     <modelVersion>4.0.0</modelVersion>
-
+    <!--    Ne moraÅ¡ ruÄno skidati JAR-ove i stavljati ih u classpath.
+    U pom.xml definiraÅ¡ koje biblioteke tvoj projekt koristi (npr. Hibernate, SQLite JDBC, Lombok).
+    Maven automatski preuzima toÄne verzije tih biblioteka s centralnog repozitorija.
+    TakoÄ‘er automatski rjeÅ¡ava transitive dependencies, tj. sve biblioteke koje tvoje biblioteke koriste.
+    -->
     <groupId>org.example</groupId>
     <artifactId>delivery-service</artifactId>
     <version>1.0-SNAPSHOT</version>
@@ -16,36 +20,78 @@
 
     <dependencies>
 
+        <!--
+        Hibernate core: glavna biblioteka za ORM (Object-Relational Mapping)
+        OmoguÄ‡ava mapiranje Java objekata na baze podataka
+        Anotacije:
+        - (preko Hibernate) @Entity, @Table, @Id, @GeneratedValue, @Column, @OneToMany, @ManyToOne, @JoinColumn, ...
+        -->
         <dependency>
             <groupId>org.hibernate.orm</groupId>
             <artifactId>hibernate-core</artifactId>
             <version>6.3.0.Final</version>
         </dependency>
 
+        <!--
+Jakarta Persistence API: standardni API za ORM u Javi
+Hibernate implementira ovaj API
+Anotacije ukljuÄuju:
+- @Entity (klasa je entitet / tablica)
+- @Table (specifiÄno ime tablice)
+- @Id (primarni kljuÄ)
+- @GeneratedValue (strategija generiranja ID-a)
+- @Column (specifikacija kolone, nullable, length)
+- @OneToMany, @ManyToOne (veze izmeÄ‘u entiteta)
+- @JoinColumn (foreign key kolona)
+-->
         <dependency>
             <groupId>jakarta.persistence</groupId>
             <artifactId>jakarta.persistence-api</artifactId>
             <version>3.1.0</version>
         </dependency>
 
+        <!--
+SQLite JDBC driver: omoguÄ‡ava povezivanje s SQLite bazom podataka preko JDBC-a
+Nema specifiÄnih anotacija, koristi se samo za konekciju i izvrÅ¡avanje SQL-a
+-->
+
         <dependency>
             <groupId>org.xerial</groupId>
             <artifactId>sqlite-jdbc</artifactId>
             <version>3.43.2.0</version>
         </dependency>
 
+        <!--
+Hibernate community dialects: dodaje podrÅ¡ku za razne SQL dijalekte (SQLite, MySQL, PostgreSQL, itd.)
+Nema dodatnih anotacija, ali omoguÄ‡ava Hibernate-u da zna kako generirati SQL za razliÄite baze
+-->
+
         <dependency>
             <groupId>org.hibernate.orm</groupId>
             <artifactId>hibernate-community-dialects</artifactId>
             <version>6.3.0.Final</version>
         </dependency>
 
+        <!--
+SLF4J Simple: logging framework
+Nema anotacija, koristi se za ispis logova iz Hibernate-a i ostalih biblioteka
+-->
         <dependency>
             <groupId>org.slf4j</groupId>
             <artifactId>slf4j-simple</artifactId>
             <version>2.0.9</version>
         </dependency>
 
+        <!--
+Lombok: olakÅ¡ava pisanje Java koda (generira gettere, settere, konstruktore itd.)
+Scope 'provided' znaÄi da Ä‡e biti potrebna samo za kompajliranje, ne za runtime
+Anotacije ukljuÄuju:
+- @Getter (generira getter metode)
+- @Setter (generira setter metode)
+- @NoArgsConstructor (generira konstruktor bez argumenata)
+- @AllArgsConstructor (generira konstruktor sa svim poljima)
+- @Builder (builder pattern za kreiranje objekata)
+-->
         <dependency>
             <groupId>org.projectlombok</groupId>
             <artifactId>lombok</artifactId>
@@ -56,4 +102,4 @@
 
     </dependencies>
 
-</project>
\ No newline at end of file
+</project>
